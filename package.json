    "@arcblock/did-auth-storage-nedb": "^1.7.1",
    "@arcblock/did-connect": "^2.13.45",
    "@arcblock/jwt": "^1.20.11",
    "@arcblock/ux": "^2.13.45",
    "@arcblock/vc": "^1.20.11",
    "@blocklet/logger": "^1.16.43",
    "@blocklet/meta": "^1.16.43",
    "@blocklet/sdk": "^1.16.43",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "^5.0.0-alpha.176",
    "@mui/material": "^5.17.1",
    "@nedb/core": "^2.1.5",
    "@ocap/client": "^1.20.11",
    "@ocap/contract": "^1.20.11",
    "@ocap/mcrypto": "^1.20.11",
    "@ocap/util": "^1.20.11",
    "@ocap/wallet": "^1.20.11",
    "axios": "^1.9.0",
    "body-parser": "^1.20.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "flat": "^5.0.2",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "material-table": "^2.0.6",
    "notistack": "^3.0.2",
    "number-precision": "^1.6.0",
    "prop-types": "^15.8.1",
    "qrcode.react": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^5.3.4",
    "react-use": "^17.6.0",
    "serve-favicon": "^2.5.0",
    "url-join": "^4.0.1"
  },
  "scripts": {
    "lint": "eslint api",
    "clean": "rm -rf .blocklet",
    "start": "nodemon -w api api/dev.js",
    "bundle": "npm run clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "vite build",
    "bundle:server": "blocklet bundle --compact --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "update:deps": "npx -y taze latest -r -w -n '/@abtnode|@aigne|@arcblock|@blocklet|@did-connect|@did-pay|@did-space|@nft-store|@nft-studio|@ocap|vite-plugin-blocklet/' && pnpm i && pnpm dedupe",
    "bump-version": "make bump-version"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "nodemon": "^3.1.10",
    "vite": "^6.3.5",
    "vite-plugin-blocklet": "^0.9.32",
    "vite-plugin-svgr": "^4.3.0"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "license": "Apache-2.0"
}
